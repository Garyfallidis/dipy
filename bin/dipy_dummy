#! /usr/bin/env python

from dipy.workflows.dummy import dummy_func_interface
from dipy.fixes import argparse as arg

parser = arg.ArgumentParser()
parser.add_argument('input1', action='store')
parser.add_argument('input2', action='store')
parser.add_argument('output', action='store')
parser.add_argument('--param1', action='store', dest='param1')
parser.add_argument('--param2', action='store', dest='param2')


if __name__ == "__main__":

    args = parser.parse_args()

    # print(args.input1)
    # print(args.input2)
    # print(args.output)

    dummy_func_interface(args.input1, args.input2, args.output,
                         args.param1, args.param2)
